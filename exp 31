#include <stdio.h>

int main() {
    int n, frames[10], pages[50], i, j, k, faults = 0, pointer = 0, found;
    printf("Enter number of frames: ");
    scanf("%d", &n);
    printf("Enter number of pages: ");
    int np;
    scanf("%d", &np);
    printf("Enter pages sequence:\n");
    for(i = 0; i < np; i++)
        scanf("%d", &pages[i]);

    for(i = 0; i < n; i++)
        frames[i] = -1;

    for(i = 0; i < np; i++) {
        found = 0;
        for(j = 0; j < n; j++) {
            if(frames[j] == pages[i]) {
                found = 1;
                break;
            }
        }
        if(!found) {
            frames[pointer] = pages[i];
            pointer = (pointer + 1) % n;
            faults++;
            printf("Page fault: ");
            for(k = 0; k < n; k++)
                printf("%d ", frames[k]);
            printf("\n");
        }
    }
    printf("Total page faults = %d\n", faults);
    return 0;
}
//
Enter number of frames: 3
Enter number of pages: 7
Enter pages sequence:
1 2 3 4 1 2 5
output:
Page fault: 1 -1 -1 
Page fault: 1 2 -1 
Page fault: 1 2 3 
Page fault: 4 2 3 
Page fault: 4 1 3 
Page fault: 4 1 2 
Page fault: 5 1 2 
Total page faults = 6
