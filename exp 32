#include <stdio.h>

int main() {
    int n, pages[50], frames[10], lru[10], i, j, k, np, faults = 0, time = 0, found;
    printf("Enter number of frames: ");
    scanf("%d", &n);
    printf("Enter number of pages: ");
    scanf("%d", &np);
    printf("Enter pages sequence:\n");
    for(i = 0; i < np; i++)
        scanf("%d", &pages[i]);

    for(i = 0; i < n; i++) {
        frames[i] = -1;
        lru[i] = 0;
    }

    for(i = 0; i < np; i++) {
        time++;
        found = 0;
        for(j = 0; j < n; j++) {
            if(frames[j] == pages[i]) {
                lru[j] = time;
                found = 1;
                break;
            }
        }
        if(!found) {
            int min = lru[0], idx = 0;
            for(j = 0; j < n; j++) {
                if(frames[j] == -1) {
                    idx = j;
                    break;
                }
                if(lru[j] < min) {
                    min = lru[j];
                    idx = j;
                }
            }
            frames[idx] = pages[i];
            lru[idx] = time;
            faults++;
            printf("Page fault: ");
            for(k = 0; k < n; k++)
                printf("%d ", frames[k]);
            printf("\n");
        }
    }
    printf("Total page faults = %d\n", faults);
    return 0;
}
//
Enter number of frames: 3
Enter number of pages: 8
Enter pages sequence:
7 0 1 2 0 3 0 4
output:
Page fault: 7 -1 -1 
Page fault: 7 0 -1 
Page fault: 7 0 1 
Page fault: 2 0 1 
Page fault: 2 0 1 
Page fault: 2 3 1 
Page fault: 2 3 0 
Page fault: 4 3 0 
Total page faults = 6
