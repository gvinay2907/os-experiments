#include <stdio.h>
#include <stdlib.h>

struct Block {
    int file;
    int block;
    struct Block* next;
};

int main() {
    int n, f, b;
    printf("Enter number of files: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        printf("Enter file %d block count: ", i+1);
        scanf("%d", &f);

        struct Block* head = NULL;
        struct Block* temp = NULL;

        for (int j = 0; j < f; j++) {
            printf("Enter block number: ");
            scanf("%d", &b);

            struct Block* newNode = (struct Block*)malloc(sizeof(struct Block));
            newNode->file = i+1;
            newNode->block = b;
            newNode->next = NULL;

            if (head == NULL) {
                head = newNode;
                temp = head;
            } else {
                temp->next = newNode;
                temp = temp->next;
            }
        }

        printf("\nFile %d blocks: ", i+1);
        temp = head;
        while (temp != NULL) {
            printf("%d -> ", temp->block);
            temp = temp->next;
        }
        printf("NULL\n");
    }
    return 0;
}
//
Enter number of files: 2
Enter file 1 block count: 3
Enter block number: 5
Enter block number: 6
Enter block number: 7

File 1 blocks: 5 -> 6 -> 7 -> NULL

Enter file 2 block count: 2
Enter block number: 10
Enter block number: 11

File 2 blocks: 10 -> 11 -> NULL
